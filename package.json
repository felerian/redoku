{
  "name": "Redoku",
  "version": "0.0.1",
  "private": true,
  "description": "Sudoku App build with Redux and using Godoku as backend.",
  "repository": {
    "type": "git",
    "url": "https://github.com/felerian/redoku.git"
  },
  "scripts": {
    "start": "gulp"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.14.0",
    "browserify": "^13.1.0",
    "del": "^2.2.2",
    "eslint": "^3.4.0",
    "gulp": "^3.9.1",
    "merge-stream": "^1.0.0",
    "proxy-middleware": "^0.15.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "paths": {
    "basedir": "./app",
    "js": "./app/**/*.js",
    "jsx": "./app/**/*.jsx",
    "app": "./app/js/main.jsx",
    "html": "./app/*.html",
    "css": "./app/**/*.css"
  },
  "dest": {
    "app": "main.js",
    "dist": "dist/js"
  },
  "dependencies": {
    "material-ui": "^0.15.0",
    "immutable": "^3.8.1",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-redux": "4.4.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "3.4.0"
